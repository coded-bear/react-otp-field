{
  "name": "react-otpinput",
  "version": "1.1.0",
  "description": "Customizable OTP Input component for the web built with React.js",
  "main": "lib/index.js",
  "scripts": {
    "dev": "concurrently \"npm run build:watch\" \"npm run demo:dev\"",
    "build": "babel src/lib -d lib",
    "build:watch": "babel src/lib -w -d lib",
    "format": "prettier --write \"**/*.{html,css,js,jsx}\"",
    "demo:dev": "webpack-dev-server --mode development",
    "demo:build": "webpack --mode production",
    "lint": "eslint 'src/**/*.{js,jsx}' --quiet",
    "lint:fix": "eslint 'src/**/*.{js,jsx}' --fix",
    "prepublish": "npm run build",
    "predeploy": "npm run demo:build",
    "deploy": "gh-pages -d demo"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/coded-bear/react-otpinput.git"
  },
  "keywords": [
    "otp",
    "otp input",
    "react otp input"
  ],
  "typings": "typings/index.d.ts",
  "author": "Łukasz Warchoł (https://lukaszwarchol.pl/)",
  "homepage": "https://coded-bear.github.io/react-otpinput",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/coded-bear/react-otpinput/issues"
  },
  "devDependencies": {
    "@babel/cli": "^7.15.7",
    "@babel/core": "^7.15.8",
    "@babel/eslint-parser": "^7.15.8",
    "@babel/preset-env": "^7.15.8",
    "@babel/preset-react": "^7.14.5",
    "babel-loader": "^8.2.3",
    "concurrently": "^6.3.0",
    "css-loader": "^6.5.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.25.2",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.26.1",
    "eslint-plugin-react-hooks": "^4.2.0",
    "gh-pages": "^3.2.3",
    "html-webpack-plugin": "^5.5.0",
    "prettier": "^2.4.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "sass": "^1.43.4",
    "sass-loader": "^12.3.0",
    "style-loader": "^3.3.1",
    "webpack": "^5.60.0",
    "webpack-cli": "^4.9.1",
    "webpack-dev-server": "^4.3.1"
  }
}
